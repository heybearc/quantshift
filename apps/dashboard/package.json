{
  "name": "quantshift-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "prisma generate && next build",
    "start": "next start --port 3000",
    "lint": "next lint",
    "prisma:generate": "prisma generate",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:smoke": "playwright test tests/smoke-test.spec.ts",
    "test:smoke:quick": "playwright test tests/smoke-test.spec.ts --workers=1",
    "test:report": "playwright show-report test-results/html-report",
    "test:debug": "playwright test --debug"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "next": "14.2.33",
    "react": "^18",
    "react-dom": "^18",
    "recharts": "^2.15.4",
    "redis": "^5.10.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.19.27",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "autoprefixer": "^10.4.23",
    "eslint": "^8",
    "eslint-config-next": "14.2.33",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3"
  }
}
